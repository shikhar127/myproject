{
  "name": "LoanNudgeFlows",
  "displayName": "Loan Nudge Flows",
  "description": "Demonstration of loan nudge flows with contextual messaging",
  "version": "1.0.0",
  "expo": {
    "name": "Loan Nudge Flows",
    "slug": "loan-nudge-flows",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.loannudgeflows"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.loannudgeflows",
      "permissions": [
        "READ_SMS",
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
